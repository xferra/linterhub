{
    "$schema": "./../.schema/deps.json",
    "id": "eslint",
    "requirements": [
        [
            {
                "manager": "pacman",
                "package": "npm"
            },
            {
                "manager": "npm",
                "package": "eslint",
                "version": "4.12.1"
            },
            {
                "manager": "npm",
                "package": "eslint-config-google"
            },
            {
